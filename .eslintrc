{
	"env": {
		"browser": true,
		"node": true,
		"es6": true,
		"jest": true
	},
	"plugins": [
		"react",
		"prettier"
	],
	"extends": [
		"uwm",
		"prettier",
		"prettier/react"
	],
	"rules": {
		"prettier/prettier": [
			"error",
			{
				"trailingComma": "es5",
				"singleQuote": true,
				"printWidth": 120,
				"useTabs": true
			}
		]
	}
}
